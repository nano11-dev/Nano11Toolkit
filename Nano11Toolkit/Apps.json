    "QueryKey": "HKEY_CURRENT_USER\\Software\\Policies\\Microsoft\\Windows\\CloudContent",
    "QueryValue": "DisableSpotlightCollectionOnDesktop",
    "EnabledOutput": "0x0",
    "Enabled": false
  },
